    "eslint-plugin-svelte3": "^3.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0"
  },
  "type": "module",
  "dependencies": {}
}
